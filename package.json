{
  "name": "angular-crud-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:solid": "ng serve solid",
    "build": "ng build",
    "predocs": "rm -rf ./docs",
    "docs": "ng build --base-href /angular-crud-demo/",
    "postdocs": "mv ./dist/fluid/browser ./docs && cp ./docs/index.html ./docs/404.html",
    "lint": "ng lint --fix",
    "format": "prettier --write ."
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.12",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.12",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.9",
    "@angular/cli": "^20.3.9",
    "@angular/compiler-cli": "^20.3.0",
    "angular-eslint": "20.5.2",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "ng-packagr": "^20.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.3"
  }
}
